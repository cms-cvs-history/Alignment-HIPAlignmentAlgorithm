  include "Alignment/CommonAlignmentProducer/data/AlignmentProducer.cff"
  include "CondCore/DBCommon/data/CondDBSetup.cfi"

  service = MessageLogger
  {
    untracked vstring destinations = {'cout'}

    untracked PSet cout = { untracked string threshold = 'WARNING' }
  }

  es_source align = PoolDBESSource
  {
    using CondDBSetup

    string connect = "frontier://cms_conditions_data/CMS_COND_20X_ALIGNMENT"
    string timetype = "runnumber"

    VPSet toGet =
    {
      { string record = "GlobalPositionRcd"        string tag = "IdealGeometry" },
      { string record = "TrackerAlignmentRcd"      string tag = "TrackerSurveyLASCosmicsScenario200_mc" },
      { string record = "TrackerAlignmentErrorRcd" string tag = "TrackerSurveyLASCosmicsScenarioErrors200_mc" }
    }
  }

#  replace AlignmentProducer.MisalignmentScenario = { using TrackerSurveyLASCosmicsScenario }
#  replace AlignmentProducer.doMisalignmentScenario = true
  replace AlignmentProducer.applyDbAlignment = true
  replace AlignmentProducer.ParameterBuilder.Selector.alignParams = {"PixelHalfBarrelDets,111111"}
